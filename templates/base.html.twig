<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>

		<!-- Font-Awesome CSS -->
		<link
		rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css"/>

		<!-- Axentix CSS -->
		<link
		rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.2.1/dist/css/axentix.min.css"/>

		<!-- Global CSS -->
		<link rel="stylesheet" href="{{ asset('/css/global.css')}}"/>
        
        {% block stylesheets %}
			{#{{ encore_entry_link_tags('app') }}#}
		{% endblock %}

		<title>
			{% block title %} {{NameProject}} {% endblock %}
		</title>
		
		{% block javascripts %}
			{#{{ encore_entry_script_tags('app') }}#}
		{% endblock %}
	</head>
	<body class="layout with-sidenav">

		<header>
			{% include "includes/navbar.html.twig" %}
		</header>

		{# {% include "includes/sidenav.html.twig" %} #}

		<main>
        
			<!-- Code here the main content -->
            {% block body %}{% endblock %}

            {# <button data-target="example-sidenav" class="btn rounded-1 press amaranth dark-1 sidenav-trigger hide-md-up">
                Open sidenav
            </button> #}

		</main>

		{% include "includes/footer.html.twig" %}

		<script src="https://cdn.jsdelivr.net/npm/axentix@1.3.3/dist/js/axentix.min.js"></script>

	</body>
</html>
